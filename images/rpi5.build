################################################################################################
##
## Build file for a QNX operating system
##
################################################################################################

[-optional]
[+keeplinked]
[image=0x80000]
[virtual=aarch64le,raw -compress] boot = {
    startup-bcm2712-rpi5 -v -u reg -a -W 5000 
    PATH=/proc/boot:/sbin:/bin:/usr/bin:/usr/sbin:/usr/libexec LD_LIBRARY_PATH=/proc/boot:/lib:/usr/lib:/lib/dll:/lib/dll/pci procnto-smp-instr -v
}

[+script] startup-script = {
    SYSNAME=nto
    TERM=qansi
    ENV=/etc/profile

    procmgr_symlink ../../proc/boot/ldqnx-64.so.2 /usr/lib/ldqnx-64.so.2

    ## Needed for ksh to run the echo command below
    pipe

    display_msg " "
    ksh -c "echo Welcome to QNX $(uname -r) on $(uname -m) !"
    display_msg " "

    slogger2
    dumper
    mqueue
    random 


    ############################################################################################
    ## WatchDog utility
    ## If startup is given -W parameter then the wdtkick utility MUST be uncommented below
    ############################################################################################
    display_msg "Starting WatchDog ..."
    wdtkick -W0x24:0x5A028E4C -W0x1c:0x5A000020




    ############################################################################################
    ## PCIe driver
    ############################################################################################
    ksh /proc/boot/pci_server_start.sh


    #######################################################################
    ## Debug console serial driver
    #######################################################################
    display_msg "Starting serial driver (/dev/ser10)"
    devc-serpl011-rpi5 -b115200 -c44236800 -e -F -u10 -D 0x107d001000^2,153


    #######################################################################
    ## SPI driver
    #######################################################################
    display_msg "Starting SPI driver"
    gpio-rp1 set 7-8 a0 pu
    gpio-rp1 set 9-11 a0 pd

    spi-dwc -c /etc/system/config/spi/spi.conf



    #######################################################################
    ## SD memory card / eMMC driver
    ## Note: "bmstr_base" option is required for busmaster memory
    ## address translation
    #######################################################################
    display_msg "Starting SDMMC driver (/dev/sd0)"
    sh -c "devb-sdmmc-bcm2712 sdio addr=0x1000fff000,irq=305 disk name=sd > /dev/null 2>&1"
    waitfor /dev/sd0



    #######################################################################
    ## USB OTG Host Controller driver
    #######################################################################
    ksh /proc/boot/usb_start.sh


    ############################################################################################
    ## Storage service
    ############################################################################################
    /proc/boot/.storage-server.sh


    ############################################################################################
    ## Network driver
    ############################################################################################
    ksh /proc/boot/net_start.sh


    ############################################################################################
    ## Customize startup components
    ############################################################################################
    ksh /proc/boot/customize_startup.sh


    # ksh /proc/boot/uart4_console.sh
    ksh /proc/boot/i2c_start.sh
    ksh /proc/boot/fan_start.sh


    ############################################################################################
    ## Start the main shell
    ############################################################################################
    reopen /dev/ser10
    display_msg "Starting shell ..."
    [+session] ksh &
}


